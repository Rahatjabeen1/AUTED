<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    style="@style/groupStylegray_101">
    <LinearLayout android:id="@+id/linearRowpolygontwo"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylegray_100">

        <ImageView
            android:id="@+id/imagePolygonTwo"
            android:layout_width="@dimen/_65pxh"
            android:layout_height="@dimen/_65pxh"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/_14pxh"
            android:layout_marginTop="@dimen/_150pxh"
            android:layout_marginBottom="@dimen/_32pxv"
            android:src="@drawable/img_polygon2" />

        <LinearLayout
            android:id="@+id/linearColumnillusone"
            android:layout_width="480dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="30dp"
            android:layout_marginBottom="@dimen/_24pxv"
            android:layout_weight="0.89"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageImageNine"
                android:layout_width="@dimen/_750pxh"
                android:layout_height="192dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_marginTop="@dimen/_86pxv"
                android:layout_marginEnd="@dimen/_26pxh"
                android:src="@drawable/img_image9" />

            <TextView
                android:id="@+id/txtDemoFour"
                style="@style/txtInterextrabold26"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:ellipsize="end"
                android:gravity="center"
                android:letterSpacing="0.33"
                android:singleLine="true"
                android:text="@string/msg_progress_tracki" />

            <LinearLayout
                android:id="@+id/linearRowellipsenine"
                android:layout_width="196dp"
                android:layout_height="30sp"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="@dimen/_277pxh"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_marginEnd="@dimen/_232pxh"
                android:orientation="horizontal">

                <View
                    android:id="@+id/viewEllipseNine"
                    android:layout_width="@dimen/_10pxh"
                    android:layout_height="@dimen/_10pxh"
                    android:layout_gravity="center"
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_4_5" />

                <View
                    android:id="@+id/viewEllipseEight"
                    android:layout_width="@dimen/_11pxh"
                    android:layout_height="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_4_5"
                    android:gravity="center" />

                <View
                    android:id="@+id/viewEllipseTen"
                    android:layout_width="@dimen/_11pxh"
                    android:layout_height="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_4_5"
                    android:gravity="center" />

                <View
                    android:id="@+id/viewEllipseEleven"
                    android:layout_width="@dimen/_10pxh"
                    android:layout_height="@dimen/_10pxh"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:background="@drawable/rectangle_bg_indigo_800_radius_5" />
            </LinearLayout>
        </LinearLayout>

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnSignup"
            style="@style/btnSolidRounded"
            android:layout_width="@dimen/_160pxh"
            android:layout_height="@dimen/_100pxv"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/_150pxh"
            android:gravity="center"
            android:paddingStart="@dimen/_11pxh"
            android:paddingTop="@dimen/_6pxv"
            android:paddingEnd="@dimen/_10pxh"
            android:paddingBottom="@dimen/_6pxv"
            android:singleLine="true"
            android:text="@string/lbl_signup2"
            android:textAllCaps="false" />
    </LinearLayout>
</RelativeLayout>
