<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    style="@style/groupStylegray_101">

    <ImageView
        android:id="@+id/imagePolygonTwo"
        android:layout_width="84dp"
        android:layout_height="80dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_gravity="center"
        android:layout_marginStart="@dimen/_14pxh"
        android:layout_marginTop="306dp"
        android:layout_marginBottom="25dp"
        android:scaleType="fitXY"
        android:src="@drawable/img_polygon2" />

    <ImageView
        android:id="@+id/imageILLUSTwo"
        android:layout_width="452dp"
        android:layout_height="283dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_gravity="center"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="179dp"
        android:scaleType="fitXY"
        android:src="@drawable/img_illus2" />


    <TextView
        android:id="@+id/txtPrice"
        style="@style/txtInterextrabold26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_gravity="center"
        android:layout_marginEnd="166dp"
        android:layout_marginBottom="108dp"
        android:letterSpacing="0.23"
        android:singleLine="true"
        android:text="@string/msg_play_games_gai" />

    <LinearLayout
        android:id="@+id/linearRowellipsenine"
        android:layout_width="133dp"
        android:layout_height="33dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_gravity="center_horizontal"
        android:layout_marginStart="177dp"
        android:layout_marginEnd="421dp"
        android:layout_marginBottom="76dp"
        android:orientation="horizontal">

        <View
            android:id="@+id/viewEllipseEleven"
            android:layout_width="@dimen/_10pxh"
            android:layout_height="@dimen/_10pxh"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/_5pxh"
            android:layout_marginBottom="@dimen/_1pxv"
            android:background="@drawable/rectangle_bg_bluegray_100_radius_4_5" />

        <View
            android:id="@+id/viewEllipseNine"
            android:layout_width="@dimen/_11pxh"
            android:layout_height="@dimen/_11pxh"
            android:layout_gravity="center"
            android:background="@drawable/rectangle_bg_indigo_800_radius_5" />

        <View
            android:id="@+id/viewEllipseEight"
            android:layout_width="@dimen/_10pxh"
            android:layout_height="@dimen/_10pxh"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/_3pxh"
            android:layout_marginBottom="@dimen/_1pxv"
            android:background="@drawable/rectangle_bg_bluegray_100_radius_4_5" />

        <View
            android:id="@+id/viewEllipseTen"
            android:layout_width="@dimen/_10pxh"
            android:layout_height="@dimen/_10pxh"
            android:layout_gravity="center"
            android:layout_marginStart="@dimen/_5pxh"
            android:layout_marginBottom="@dimen/_1pxv"
            android:background="@drawable/rectangle_bg_bluegray_100_radius_4_5" />


    </LinearLayout>

    <ImageView
        android:id="@+id/imagePolygonOne"
        android:layout_width="87dp"
        android:layout_height="82dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_gravity="center"
        android:layout_marginTop="312dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="17dp"
        android:scaleType="fitXY"
        android:src="@drawable/img_polygon1" />

</RelativeLayout>
