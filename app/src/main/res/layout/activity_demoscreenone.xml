<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    style="@style/groupStylegray_101">
    <LinearLayout android:id="@+id/linearDemoscreenOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylegray_100">

        <LinearLayout
            android:id="@+id/linearColumnillusone"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginStart="@dimen/_121pxh"
            android:layout_marginBottom="@dimen/_24pxv"
            android:layout_weight="0.89"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageILLUSOne"
                android:layout_width="325dp"
                android:layout_height="251dp"
                android:layout_gravity="center"
                android:layout_marginStart="@dimen/_104pxh"
                android:layout_marginEnd="@dimen/_104pxh"
                android:src="@drawable/img_illus1" />

            <LinearLayout
                android:id="@+id/linearColumnprice"
                android:layout_width="wrap_content"
                android:layout_gravity="center"
                android:layout_height="145sp"
                android:layout_marginTop="7sp"
                android:layout_marginEnd="10sp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtPrice"
                    style="@style/txtInterextrabold26"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:letterSpacing="0.23"
                    android:singleLine="true"
                    android:text="@string/msg_play_games_gai" />

                <LinearLayout
                    android:id="@+id/linearRowellipsenine"
                    android:layout_width="120sp"
                    android:layout_height="30sp"
                    android:layout_marginStart="@dimen/_237pxh"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_marginEnd="@dimen/_232pxh"
                    android:layout_gravity="center_horizontal"
                    android:orientation="horizontal">

                    <View
                        android:id="@+id/viewEllipseNine"
                        android:layout_width="@dimen/_11pxh"
                        android:layout_height="@dimen/_11pxh"
                        android:layout_gravity="center"
                        android:background="@drawable/rectangle_bg_indigo_800_radius_5" />

                    <View
                        android:id="@+id/viewEllipseEight"
                        android:layout_gravity="center"
                        android:layout_width="@dimen/_10pxh"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_3pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:background="@drawable/rectangle_bg_bluegray_100_radius_4_5" />

                    <View
                        android:id="@+id/viewEllipseTen"
                        android:layout_gravity="center"
                        android:layout_width="@dimen/_10pxh"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:background="@drawable/rectangle_bg_bluegray_100_radius_4_5" />

                    <View
                        android:id="@+id/viewEllipseEleven"
                        android:layout_gravity="center"
                        android:layout_width="@dimen/_10pxh"
                        android:layout_height="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:background="@drawable/rectangle_bg_bluegray_100_radius_4_5" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <ImageView android:id="@+id/imagePolygonOne"
            android:layout_height="@dimen/_65pxh"
            android:layout_marginBottom="@dimen/_32pxv"
            android:layout_marginEnd="@dimen/_14pxh"
            android:layout_marginTop="@dimen/_150pxh"
            android:layout_gravity="center"
            android:layout_width="@dimen/_65pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_polygon1"/>
    </LinearLayout>
</RelativeLayout>
