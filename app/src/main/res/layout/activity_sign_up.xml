<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    tools:context=".SignUp"
    style="@style/groupStylegray_101">

    <ImageView
        android:layout_width="110dp"
        android:layout_height="101dp"
        android:layout_marginStart="-18dp"
        android:src="@drawable/img_topleft"/>

    <ImageView
        android:layout_width="144dp"
        android:layout_height="126dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="2dp"
        android:layout_marginTop="356dp"
        android:layout_marginEnd="420dp"
        android:layout_marginBottom="-3dp"
        android:src="@drawable/img_bottomleft" />

    <ImageView
        android:layout_width="121dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="635dp"
        android:layout_marginTop="324dp"
        android:layout_marginEnd="420dp"
        android:src="@drawable/img_bottomright" />

    <ImageView
        android:layout_width="117dp"
        android:layout_height="61dp"
        android:layout_marginStart="615dp"
        android:layout_marginTop="-1dp"
        android:layout_marginEnd="420dp"
        android:src="@drawable/img_topright"/>


    <ImageView
        android:layout_width="125dp"
        android:layout_height="103dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="289dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="321dp"
        android:src="@drawable/img_logo" />

    <TextView
        android:layout_width="283dp"
        android:layout_height="54dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="40dp"
        android:layout_marginTop="21dp"
        android:text="EMAIL"
        android:textSize="16sp" />

    <EditText
        android:id="@+id/email"
        style="@style/etSolid"
        android:layout_width="303dp"
        android:layout_height="92dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_gravity="top|start"
        android:layout_marginStart="44dp"
        android:layout_marginTop="67dp"
        android:layout_marginEnd="384dp"
        android:layout_marginBottom="252dp"
        android:background="@drawable/rectangle_bg_white_a700"
        android:drawableStart="@drawable/img_vector"
        android:drawablePadding="@dimen/_7pxh"
        android:ellipsize="end"
        android:hint="email"
        android:inputType="textEmailAddress"
        android:paddingStart="@dimen/_8pxh"
        android:paddingTop="@dimen/_3pxv"
        android:paddingEnd="@dimen/_30pxh"
        android:paddingBottom="@dimen/_5pxv"
        android:singleLine="true"
        android:textColorHint="@color/gray_500" />

    <TextView
        android:layout_width="308dp"
        android:layout_height="52dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="42dp"
        android:layout_marginTop="159dp"
        android:layout_marginEnd="381dp"
        android:layout_marginBottom="200dp"
        android:text="USERNAME"
        android:textSize="16sp" />

    <EditText
        android:id="@+id/USERNAME"
        style="@style/etSolid"
        android:layout_width="306dp"
        android:layout_height="75dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_gravity="top|start"
        android:layout_marginStart="42dp"
        android:layout_marginTop="210dp"
        android:layout_marginEnd="383dp"
        android:background="@drawable/rectangle_bg_white_a700"
        android:drawableStart="@drawable/img_user"
        android:drawablePadding="9dp"
        android:hint="username"
        android:paddingStart="@dimen/_8pxh"
        android:paddingTop="@dimen/_3pxv"
        android:paddingEnd="@dimen/_30pxh"
        android:paddingBottom="@dimen/_5pxv"
        android:singleLine="true"
        android:textColorHint="@color/gray_500" />

    <TextView
        android:layout_width="262dp"
        android:layout_height="47dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="424dp"
        android:layout_marginTop="41dp"
        android:layout_marginEnd="45dp"
        android:text="PASSWORD"
        android:textSize="16sp" />

    <EditText
        android:id="@+id/PASSWORD"
        style="@style/etSolid"
        android:layout_width="270dp"
        android:layout_height="67dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_gravity="top|start"
        android:layout_marginStart="429dp"
        android:layout_marginTop="92dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="@dimen/_173pxv"
        android:background="@drawable/rectangle_bg_white_a700"
        android:drawableStart="@drawable/img_lock"
        android:drawablePadding="@dimen/_7pxh"
        android:drawableTint="#FFA9A8"
        android:ellipsize="end"
        android:paddingStart="@dimen/_8pxh"
        android:paddingTop="@dimen/_3pxv"
        android:paddingEnd="@dimen/_30pxh"
        android:paddingBottom="@dimen/_5pxv"
        android:singleLine="true"
        android:hint="password"
        android:textColorHint="@color/gray_500"
         />

    <TextView
        android:layout_width="290dp"
        android:layout_height="57dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="426dp"
        android:layout_marginTop="169dp"
        android:layout_marginEnd="15dp"
        android:text="CONFIRM PASSWORD"
        android:textSize="16sp" />

    <EditText
        android:id="@+id/confirmpassword"
        style="@style/etSolid"
        android:layout_width="292dp"
        android:layout_height="71dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_gravity="top|start"
        android:layout_marginStart="424dp"
        android:layout_marginTop="218dp"
        android:layout_marginEnd="15dp"
        android:background="@drawable/rectangle_bg_white_a700"
        android:drawableStart="@drawable/img_lock"
        android:drawablePadding="@dimen/_7pxh"
        android:drawableTint="#FFA9A8"
        android:ellipsize="end"
        android:hint="confirmpassword"
        android:paddingStart="@dimen/_8pxh"
        android:paddingTop="@dimen/_3pxv"
        android:paddingEnd="@dimen/_30pxh"
        android:paddingBottom="@dimen/_5pxv"
        android:singleLine="true"
        android:textColorHint="@color/gray_500" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/signup"
        style="@style/btnSolid_2"
        android:layout_width="284dp"
        android:layout_height="95dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_gravity="top|end"
        android:layout_marginStart="47dp"
        android:layout_marginTop="286dp"
        android:layout_marginEnd="400dp"
        android:layout_marginBottom="30dp"
        android:gravity="center"
        android:paddingStart="@dimen/_30pxh"
        android:paddingTop="@dimen/_11pxv"
        android:paddingEnd="@dimen/_30pxh"
        android:paddingBottom="@dimen/_11pxv"
        android:singleLine="true"
        android:text="@string/lbl_sign_up"
        android:textAllCaps="false" />

    <TextView
        android:id="@+id/txt"
        style="@style/txtInterregular16"
        android:layout_width="368dp"
        android:layout_height="55dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_gravity="bottom|end"
        android:layout_marginStart="30dp"
        android:layout_marginTop="306dp"
        android:layout_marginEnd="14dp"
        android:layout_marginBottom="49dp"
        android:ellipsize="end"
        android:singleLine="true"
        android:text="HAVE AN ACCOUNT ALREADY? LOGIN" />

</RelativeLayout>



